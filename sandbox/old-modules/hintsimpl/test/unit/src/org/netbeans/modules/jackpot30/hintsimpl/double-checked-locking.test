%%TestCase simple
package test;
public class Test {
    private static Object TEST;
    public static Object get() {
        if (TEST == null) {
            synchronized (Test.class) {
                if (TEST == null) {
                    TEST = new Object();
                }
            }
        }

        return TEST;
    }
}
