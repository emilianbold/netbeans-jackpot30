%%TestCase positive-equals-1
package test;
public class Test {
    private boolean test(Object o1, Object o2) {
        return (o1 == null) ? (o2 != null) : !o1.equals(o2);
    }
}
%%=>
package test;

import java.util.Objects;

public class Test {
    private boolean test(Object o1, Object o2) {
        return Objects.equals(o1, o2);
    }
}
